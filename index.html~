	<script src="./nav.js"></script>
	<script>
		var formClicked = false;
		var internalRoute = "about";
		function load(){
			route("about");
			document.querySelector(".title").classList.add("title-animation");
			document.querySelector(".fab1").classList.add("fab1-animation-in");
		}
		function scroll(){
			var scroll1 = document.querySelector(".projects").getBoundingClientRect().top;
			var scroll2 = document.querySelector(".contact").getBoundingClientRect().top;
			var scrollTop = document.body.scrollTop;
			var height = window.innerHeight;
			if (scrollTop === 0){
				document.querySelector(".header").classList.remove("header-animation");
				document.querySelector(".title").classList.add("title-animation");
				document.querySelector(".fab1").classList.add("fab1-animation-in");
				document.querySelector(".fab2").classList.remove("fab2-animation-in");
				document.getElementById("about").style.background = "#76FF03";
				document.getElementById("about").style.color = "#212121";
				document.getElementById("projects").style.color = "#FAFAFA";
				document.getElementById("projects").style.background = "transparent";
				document.getElementById("contact").style.background = "transparent";
				document.getElementById("contact").style.color = "#FAFAFA";
			}
			if (scrollTop > 100){
				document.querySelector(".header").classList.add("header-animation");
				document.querySelector(".title").classList.remove("title-animation");
			}
			if (scroll1 <= 0){
				document.querySelector(".fab1").classList.remove("fab1-animation-in");
				document.getElementById("about").style.background = "transparent";
				document.getElementById("about").style.color = "#FAFAFA";
				document.getElementById("contact").style.background = "transparent";
				document.getElementById("contact").style.color = "#FAFAFA";
				document.getElementById("projects").style.background = "#76FF03";
				document.getElementById("projects").style.color = "#212121";
			}
			if (scroll2 == 0){
				document.querySelector(".fab2").classList.remove("fab2-animation-in");
				document.querySelector(".fab3").classList.add("fab3-animation-in");
				document.querySelector(".contactTitle").classList.add("contactTitle-animation");
				document.querySelector(".contact-logo").classList.add("contact-logo-animation");
				document.querySelector(".contactForm").classList.add("contactForm-animation");
				document.getElementById("about").style.background = "transparent";
				document.getElementById("projects").style.background = "transparent";
				document.getElementById("projects").style.color = "#FAFAFA";
				document.getElementById("contact").style.background = "#76FF03";
				document.getElementById("contact").style.color = "#212121";
			}
			if ((scrollTop >= (height-56)) && (scroll2 >= 56)){
				document.querySelector(".fab2").classList.add("fab2-animation-in");
				document.querySelector(".fab3").classList.remove("fab3-animation-in");
				if (!formClicked){
					document.querySelector(".contactTitle").classList.remove("contactTitle-animation");
					document.querySelector(".contact-logo").classList.remove("contact-logo-animation");
					document.querySelector(".contactForm").classList.remove("contactForm-animation");
				}
			}
		}
		function scrollToProjects(){
			var scrollTop = document.body.scrollTop;
		  var pos = scrollTop;
			var i = 0;
		  var id = setInterval(frame, 10);
		  function frame() {
					var scroll1 = document.querySelector(".projects").getBoundingClientRect().top;
		      if (scroll1 <= -56) {
		          clearInterval(id);
		      } else {
							i++
		          pos = pos + Math.pow(i, 2)/i; 
							window.scrollTo(0, pos);
		      }
		  }
		}
		function reverseScrollToProjects(){
			var scrollTop = document.body.scrollTop;
		  var pos = scrollTop;
			var i = 0;
		  var id = setInterval(frame, 10);
		  function frame() {
					var scroll1 = document.querySelector(".projects").getBoundingClientRect().top;
		      if (scroll1 >= -56) {
		          clearInterval(id);
		      } else {
							i++
		          pos = pos - Math.pow(i, 2)/i; 
							window.scrollTo(0, pos);
		      }
		  }
		}
		function scrollToContact(){
			var scrollTop = document.body.scrollTop;
		  var pos = scrollTop;
			var i = 0;
		  var id = setInterval(frame, 10);
		  function frame() {
					var scroll2 = document.querySelector(".contact").getBoundingClientRect().top;
		      if (scroll2 <= 0) {
		          clearInterval(id);
		      } else {
							i++
		          pos = pos + Math.pow(i, 2)/i; 
							window.scrollTo(0, pos);
		      }
		  }
		}
		function scrollToTop(){
			var scrollTop = document.body.scrollTop;
		  var pos = scrollTop;
			var i = 0;
		  var id = setInterval(frame, 10);
		  function frame() {
		      if (pos <= 0) {
		          clearInterval(id);
		      } else {
							i++
		          pos = pos - Math.pow(i, 2)/i; 
							window.scrollTo(0, pos);
		      }
		  }
		}
		function focusTextArea(){
			formClicked = true;
			document.querySelector(".messageUnderline").classList.add("messageUnderline-animation");
			document.querySelector(".emailUnderline").classList.remove("emailUnderline-animation");
			document.querySelector(".contactTitle").classList.add("contactTitle-animation");
			document.querySelector(".contact-logo").classList.add("contact-logo-animation");
			document.querySelector(".contactForm").classList.add("contactForm-animation");
		}
		function focusEmail(){
			formClicked = true;
			document.querySelector(".emailUnderline").classList.add("emailUnderline-animation");
			document.querySelector(".messageUnderline").classList.remove("messageUnderline-animation");
			document.querySelector(".contactTitle").classList.add("contactTitle-animation");
			document.querySelector(".contact-logo").classList.add("contact-logo-animation");
			document.querySelector(".contactForm").classList.add("contactForm-animation");
		}
		function route(destination){
			if (destination === "about"){
				document.getElementById("about").style.background = "#76FF03";
				document.getElementById("about").style.color = "#212121";
				scrollToTop();
			}
			if (destination === "projects"){
				var scrollTop = document.body.scrollTop;
				var scroll1 = document.querySelector(".projects").getBoundingClientRect().top;
				var scroll2 = document.querySelector(".contact").getBoundingClientRect().top;
				document.getElementById("projects").style.background = "#76FF03";
				document.getElementById("projects").style.color = "#212121";
				scrollToProjects();
				if (scroll1 < 0){
					reverseScrollToProjects();
				}
			}
			if (destination === "contact"){
				document.getElementById("contact").style.background = "#76FF03";
				document.getElementById("contact").style.color = "#212121";
				scrollToContact();
			}
		}
	</script>
</html>
